@inherits UmbracoTemplatePage
@{
    Layout = null;
}<!DOCTYPE html>
<!--
    TXT 2.0 by HTML5 UP
    html5up.net | twitter.com/n33co
    Free for personal and commercial use under the CCA 3.0 license (html5up.net/license)
-->
<html>
<head>
    <title>@Umbraco.Field("title", altFieldAlias: "pageName") | @Umbraco.Field("siteName", recursive: true)</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.min.js"></script>
    <link rel="stylesheet" href="/css/VelocityRocket.css" />
    <link rel="stylesheet" href="/css/glyphicons.css" />
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/ngTables.js"></script>

</head>
<body>

    <!-- /Header -->
    <header>
        @Html.Partial("MainNavigation")
    </header>


    <div class="wrapper" style="padding: 40px;">


        <div class="row" id="content-wrapper">

            <div class="row">
                <h1 class="pull-left">
                    <i class="icon-briefcase"></i>
                    <span>
                        @Umbraco.Field("title", altFieldAlias: "pageName")
                    </span>

                    <li data-ng-click="addDeal(true)" class="btn text-green ng-scope" style="margin-left: 20px;" ng-if="isPageEbedded==false">
                        <i class="icon-plus text-green"></i>
                        New @Umbraco.Field("title", altFieldAlias: "pageName")
                    </li>

                </h1>
            </div>

            <div class="row">
                @Html.Partial("Trail")
            </div>

            <div class="row">
                @RenderBody()
            </div>

        </div>

    </div>
    
</body>
</html>