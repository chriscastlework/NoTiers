        public DateTime? DateCreated {get;set;}
        public DateTime? DateLastModeified {get;set;}
        public string CreatedById {get;set;}
        public string LastModifiedById {get;set;}
        public bool AllowMultipleDealContactRoles {get;set;}
        public string DefaultOrgCurrency {get;set;}
        public DateTime? FinacialYearEndDateTime {get;set;}
        public bool NewTrialNotificationEmailSent {get;set;}
        public bool GlobalEmailPreferences {get;set;}
        public int? DefaultStc_Id {get;set;}
