        public int? CustomTableId {get;set;}
        public int EntityType {get;set;}
        public string RecordTypeId {get;set;}
        public bool AutomaticMandatoryStepProgression {get;set;}
        public bool? HideProbability {get;set;}
        public bool? HideVerifiableOutcomes {get;set;}

    }
}
