        public string CustomScript {get;set;}
        public string CustomUserScript {get;set;}
        public bool? ImportMissingUsers {get;set;}
        public bool? DailyBulkUpdateEnabled {get;set;}
        public DateTime? BulkUpdateTime {get;set;}

    }
