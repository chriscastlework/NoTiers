using System.Linq;
using CustomLogic.Core.Interfaces;
using CustomLogic.Core.Interfaces.DL;
using CustomLogic.Core.Models;

namespace CustomLogic.Services.ReceivedWebhookModelQueuesService
{
public class ReturnResultEvent : IUpdateEvent<ReceivedWebhookModelQueuesViewModel, ReceivedWebhookModelQueue>
{
    public int priority()
    {
        return 1000;
    }

    public bool Run(ReceivedWebhookModelQueuesViewModel model, ref IQueryable< ReceivedWebhookModelQueue> repository, IUnitOfWork unitOfWork, Response<ReceivedWebhookModelQueuesViewModel>result, ICoreUser user)
    {
        var updatedDbModel = unitOfWork.With<ReceivedWebhookModelQueue>().Single(c => c.Id == model.Id); // Might be a partial primary key template needs 
        var newCustomResult = ReceivedWebhookModelQueues
        Mapper.MapDbModelToViewModel(updatedDbModel);
        result.Data = newCustomResult;
        return true;
    }
}

