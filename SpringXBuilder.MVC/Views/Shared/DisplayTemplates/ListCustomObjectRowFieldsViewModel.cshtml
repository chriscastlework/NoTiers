@model IEnumerable<CustomLogic.Services.CustomObjectRowFieldsService.CustomObjectRowFieldsViewModel>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Field_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomObjectRow_Id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Field_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomObjectRow_Id)
            </td>
            <td>
                @Html.ActionLink("Edit Application", "Edit", new { id = item.Id }) |

                @Html.ActionLink("View Application", "Index", "Applet", new
               {
                   fileopen = "Applet/Load/" + item.Id
               }, null)
                |
                @Html.ActionLink("View Application", "LoadAngular/" + item.Id, "Applet" , null, null)

            </td>
        </tr>
    }
</table>
